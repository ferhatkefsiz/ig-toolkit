{
  "env": {
    "browser": true,
    "jest": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": ["@typescript-eslint"],
  "extends": ["plugin:@typescript-eslint/recommended"],
  "rules": {
    "quotes": [
      "error",
      "double"
    ],
    "@typescript-eslint/quotes": [
      "error",
      "double"
    ]
  }
}
